# My personal modifications
# Applications

# Scripts
alias h="cd ~/Scripts;./h.sh"
alias i="cd ~/.config/i3"

# Mount
alias m="sudo mount -o uid=USER /dev/sdb /mnt/android"
alias ma="jmtpfs -o allow_other ~/Mnt"
alias uma="fusermount -u ~/Mnt"

# Todo
alias todo="$HOME/bin/todo.sh"
alias ta="todo -a"
alias trr="todo -r"
alias tra="todo -r all"
alias tl="todo -l"

# Directory
alias clg="cd ~/Desktop/kgec"
alias desktop="cd ~/Desktop"
alias subu="cd ~/Desktop/subu"
alias th="thunar"
alias u="cd ~/Desktop/kgec/Unix/unix7"
alias n="cd ~/Desktop/kgec/Numerical/numerical4"
alias kgec="cd ~/Desktop/kgec"
alias g="cd ~/Desktop/kgec/git"
export EDITOR=vim


alias t="thunar ."
alias own="sudo chmod +x ./*"
alias backup="sudo bash ~/Desktop/kgec/backup.sh"
